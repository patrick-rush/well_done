<h2><%= @user.full_name %>'s Projects </h2><br>
<% @projects.each do |project| %>
    <%= link_to project.title, project_path(project) %>
    <%= render partial: "tasks/task_list", locals: {tasks: project.tasks} %>
<% end %>
<%= button_to 'New Project', new_project_path, method: :get %>
<%= button_to 'New Task', new_task_path, method: :get %>
